{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "typeRoots": [
      "../../typings",
      "../../node_modules/@types",
      "./types"
    ],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "types/**/*.ts",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "src/components/pages/entry/DescriptorsTabs/TabsFooter.mixin.ts",
    "src/components/layouts/shared/navbar/navbar.mixin.js",
    "src/components/pages/profile/modals/CropModalMixin/CropModal.mixin.js",
    "src/registerApplications.ts",
    "src/components/pages/entry/DescriptorsTabs/TabsFooter.mixin.js",
  ],
  "exclude": [
    "node_modules"
  ]
}
